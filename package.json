{
    "name": "ZBD-boardGames",
    "version": "1.0.0",
    "description": "Webapp to maintain Oracle Database",
    "main": "index.js",
    "scripts": {
        "webpack": "webpack",
        "webpack-dev-server": "webpack-dev-server",
        "prod": "concurrently \"yarn prod:front\" \"yarn prod:back\"",
        "prod:front": "yarn webpack --config ./webpack-frontend.config.js --env.mode production",
        "prod:back": "yarn webpack --config ./webpack-backend.config.js --env.mode production",
        "dev": "concurrently  \"yarn dev:front\" \"yarn dev:back\"",
        "dev:front": "yarn webpack-dev-server --config ./webpack-frontend.config.js --env.mode development",
        "dev:back": "yarn webpack --config ./webpack-backend.config.js --env.mode development"
    },
    "repository": "git@github.com:grazik/ZBD-boardGames.git",
    "author": "m_graziowski & Stanislaw Szataniak",
    "license": "ISC",
    "devDependencies": {
        "webpack-cli": "^3.1.2"
    },
    "dependencies": {
        "babel-core": "^6.26.3",
        "babel-loader": "7",
        "babel-plugin-transform-regenerator": "^6.26.0",
        "babel-plugin-transform-runtime": "^6.23.0",
        "babel-preset-env": "^1.7.0",
        "concurrently": "^4.0.1",
        "css-loader": "^1.0.1",
        "eslint": "^5.7.0",
        "eslint-config-airbnb-base": "^13.1.0",
        "eslint-plugin-import": "^2.14.0",
        "express": "^4.16.4",
        "html-webpack-plugin": "^3.2.0",
        "mini-css-extract-plugin": "^0.4.4",
        "start-server-webpack-plugin": "^2.2.5",
        "webpack": "^4.23.1",
        "webpack-dev-server": "^3.1.10",
        "webpack-merge": "^4.1.4",
        "webpack-node-externals": "^1.7.2"
    }
}
