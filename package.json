{
    "name": "ZBD-boardGames",
    "version": "1.0.0",
    "description": "Webapp to maintain Oracle Database",
    "main": "index.js",
    "scripts": {
        "webpack": "webpack",
        "webpack-dev-server": "webpack-dev-server",
        "prod": "concurrently \"yarn prod:front\" \"yarn prod:back\"",
        "prod:front": "yarn webpack --config ./webpack-frontend.config.js --env.mode production",
        "prod:back": "yarn webpack --config ./webpack-backend.config.js --env.mode production",
        "dev": "concurrently  \"yarn dev:front\" \"yarn dev:back\"",
        "dev:front": "yarn webpack-dev-server --config ./webpack-frontend.config.js --hot --env.mode development",
        "dev:back": "yarn webpack --config ./webpack-backend.config.js --env.mode development"
    },
    "repository": "git@github.com:grazik/ZBD-boardGames.git",
    "author": "m_graziowski & Stanislaw Szataniak",
    "license": "ISC",
    "devDependencies": {
        "webpack-cli": "^3.1.2"
    },
    "dependencies": {
        "@babel/core": "^7.1.6",
        "@babel/plugin-syntax-dynamic-import": "^7.0.0",
        "@babel/plugin-transform-regenerator": "^7.0.0",
        "@babel/plugin-transform-runtime": "^7.1.0",
        "@babel/preset-env": "^7.1.6",
        "@babel/runtime": "7.0.0-beta.55",
        "apollo-server-express": "^2.2.1",
        "babel-loader": "^8.0.4",
        "clean-webpack-plugin": "^0.1.19",
        "concurrently": "^4.0.1",
        "cookie-parser": "^1.4.3",
        "css-loader": "^1.0.1",
        "eslint": "^5.7.0",
        "eslint-config-airbnb-base": "^13.1.0",
        "eslint-plugin-import": "^2.14.0",
        "express": "^4.16.4",
        "file-loader": "^2.0.0",
        "graphql": "^14.0.2",
        "handlebars": "^4.0.12",
        "html-loader": "^0.5.5",
        "html-webpack-plugin": "^3.2.0",
        "jsdom": "^13.0.0",
        "lodash.merge": "^4.6.1",
        "mini-css-extract-plugin": "^0.4.4",
        "mysql": "^2.16.0",
        "node-fetch": "^2.3.0",
        "node-sass": "^4.10.0",
        "raw-loader": "^0.5.1",
        "sass-loader": "^7.1.0",
        "siema": "^1.5.1",
        "start-server-webpack-plugin": "^2.2.5",
        "style-loader": "^0.23.1",
        "webpack": "^4.23.1",
        "webpack-bundle-analyzer": "^3.0.3",
        "webpack-dev-server": "^3.1.10",
        "webpack-merge": "^4.1.4",
        "webpack-node-externals": "^1.7.2"
    }
}
